<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Automation for Sytner">
 <parameter name="serverName" value="STAGING" />

    <listeners>
